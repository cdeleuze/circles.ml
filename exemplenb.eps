    % draw circle
    x y translate
    newpath 0 0 r 0 360 arc stroke

    % fix text scale
    height width atan cos r mul 2 mul width div dup scale

    % draw centered text
    width 2 div neg height 2 div neg moveto t show

    grestore
} def


/c { % x y r c

    (mystring) cvs textincircle

} def
